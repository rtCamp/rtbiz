{
	"env": {
		"browser": true,
		"node": true
	},
	"parser": "babel-eslint",
	"rules": {
		"quotes": [
			2,
			"single"
		],
		"strict": [
			2,
			"never"
		],
		"react/jsx-uses-react": 2,
		"react/jsx-uses-vars": 2
	},
	"plugins": [
		"react"
	],
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	}
}
